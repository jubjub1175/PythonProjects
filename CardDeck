#Generate Empty Deck
	Deck = []
	# Create Spades
	i = 1
	while i < 14:
		Deck.append(str(i) + " of Spades")
		i += 1

	i = 1
	while i < 14:
		Deck.append(str(i) + " of Hearts")
		i += 1

	i = 1
	while i < 14:
		Deck.append(str(i) + " of Diamonds")
		i += 1

	i = 1
	while i < 14:
		Deck.append(str(i) + " of Clubs")
		i += 1

	print(Deck)

DeckGeneration()

